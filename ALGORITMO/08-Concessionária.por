algoritmo
declare
ctotal numerico
imposto numerico
cfabrica numerico
percdist numerico
escreva "CONCESSIONÁRIA"
escreva "Digite o custo de fábrica do veículo: "
leia cfabrica
imposto<-cfabrica*0.45
percdist<-cfabrica*0.12
ctotal<-cfabrica+imposto+percdist
escreva "O custo final ao consumidor é: "
escreva ctotal
fim_algoritmo
